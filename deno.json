{
  "tasks": {
    "start": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys src/cli.ts"
  },
  "imports": {
    "browser-id3-writer": "npm:browser-id3-writer@^6.1.0"
  },
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "lib": ["deno.window", "deno.ns", "es2020"],
    "strict": true,
    "checkJs": false,
    "noImplicitAny": true
  },
  "lint": {
    "rules": {
      "exclude": ["no-control-regex"]
    }
  }
}
